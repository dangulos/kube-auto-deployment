{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "itrm-service"
    },
    "spec": {
        "type":"LoadBalancer",
        "selector": {
            "app": "itrm-container"
        },
        "ports":[
            {
                "name": "THEIA",
                "port": 3000,
                "protocol": "TCP"
            },
            {
                "name": "TV",
                "port": 5000,
                "protocol": "TCP"
            }
        ]
    }
}
