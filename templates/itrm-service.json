{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "itrm-service",
        "labels": {
            "run": "deployment"
        }
    },
    "spec": {
        "type":"ClusterIp",
        "selector": {
            "app": "itrm-container"
        },
        "ports":[
            {
                "port": 3000,
                "protocol": "TCP"
            }
        ]
    }
}