  
{
    "kind": "Deployment",
    "spec": {
      "replicas": 1,
      "template": {
        "spec": {
          "containers": [
            {
              "image": "dangulos/itrm-container:latest",
              "name": "itrm-container",
              "ports": [
                {
                  "containerPort": 3000
                },
                {
                  "containerPort": 5000
                }
              ],
              "env":[{
                "name":"MYNAME",
                "value":"name"
              }]
            }
          ]
        },
        "metadata": {
          "labels": {
            "app": "itrm-container"
          }
        }
      },
      "selector": {
        "matchLabels": {
          "app": "itrm-container"
        }
      }
    },
    "apiVersion": "apps/v1",
    "metadata": {
      "labels": {
        "app": "itrm-container"
      },
      "name": "itrm-deployment"
    }
  }
